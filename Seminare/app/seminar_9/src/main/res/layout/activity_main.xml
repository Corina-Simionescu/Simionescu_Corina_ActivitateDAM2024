<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/adaugaCaineButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adauga caine" />

    <Button
        android:id="@+id/buttonVizualizareCaini"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vizualizare caini" />

    <Button
        android:id="@+id/buttonListaImagini"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lista imagini" />

    <Button
        android:id="@+id/buttonDateJsonASE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date json ASE" />

    <Button
        android:id="@+id/buttonVreme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vezi vremea" />

</LinearLayout>